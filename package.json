{
  "name": "trollstreet",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-watch": "^2.0.7",
    "concurrently": "^3.5.1",
    "react": "^16.3.1",
    "react-debounce-input": "^3.2.0",
    "react-dom": "^16.3.1",
    "react-morph": "^0.3.5",
    "react-scripts": "1.1.4",
    "styled-components": "^3.2.5"
  },
  "scripts": {
    "build": "react-scripts build && NODE_ENV=production npx babel --out-file ./build/server.js < ./src/server.js --presets=env",
    "deploy": "yarn build && now -e NODE_ENV=\"production\" && now alias && now scale spotify-sxsw.now.sh 1 1",
    "now-build": "true",
    "now-start": "PORT=8081 NODE_ENV=production node build/server.js",
    "server": "PORT=8081 babel-watch ./src/server.js",
    "start": "concurrently \"react-scripts start\" \"yarn server\""
  },
  "dependencies": {
    "compression": "^1.7.2",
    "express": "^4.16.3",
    "fuse.js": "^3.2.0",
    "node-html-parser": "^1.1.6",
    "request": "^2.85.0"
  },
  "proxy": {
    "/": {
      "target": "http://localhost:8081",
      "pathRewrite": {
        "^/proxy": ""
      },
      "changeOrigin": true
    }
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
